<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.knurling_dietinbody">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Knurling_DietInbody">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".InBodyWriteActivity"></activity>
        <activity android:name=".InBodyViewActivity" />
        <activity android:name=".InBodyListActivity" />
        <activity android:name=".DiaryWriteActivity"></activity>
        <activity android:name=".DiaryViewActivity" />
        <activity android:name=".DiaryListActivity" />
        <activity android:name=".PrivacyActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".SplashActivity">

        </activity>
        <activity android:name=".LoginActivity" />
    </application>

</manifest>